								myself::InstantTeleport(myself::GetCreatureFromIndex(i3), RpointX, RpointY, RpointZ );//R-point
							}
						}
					}
					
					myself.i_ai1 =0;
					myself.i_ai2 =0;
					myself.i_ai0=RegistrationState;
				}
				
				myself::AddTimerEx(TimerID,  TimerPrecisionSec*1000);
